   "web_accessible_resources": [ "image/direct.png", "icon.png", "image/direct.png", "image/loading.gif", "image/rss.png", "image/enable.png", "image/disable.png", "image/menu.svg", "image/options.png", "image/qmark.png", "image/hat.png" ]
