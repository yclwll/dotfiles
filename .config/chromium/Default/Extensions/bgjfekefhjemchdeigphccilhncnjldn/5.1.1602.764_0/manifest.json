{
   "author": "zimo",
   "background": {
      "persistent": true,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icon.png",
         "38": "icon.png"
      },
      "default_popup": "html/popup.html",
      "default_title": "smartUp"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/event.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
