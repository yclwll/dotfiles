   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "22",
   "name": "__MSG_ext_name__",
